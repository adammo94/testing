{
  "name": "ghnext",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . && stylelint \"app/**/*.styles.ts\"",
    "lint:types": "tsc --noemit"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/material": "^5.10.7",
    "@reduxjs/toolkit": "^1.8.5",
    "@types/next-redux-wrapper": "^3.0.0",
    "@types/react-redux": "^7.1.24",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-react-app": "^7.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-destructuring-newline": "^0.0.2",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-modules-newline": "^0.0.6",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-webpack-plugin": "^3.2.0",
    "firebase": "^9.10.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "next": "12.3.1",
    "next-auth": "^4.12.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.4",
    "redux": "^4.2.0",
    "styled-components": "^5.3.6",
    "stylelint": "^14.13.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^5.0.0",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "devDependencies": {
    "@types/node": "18.7.23",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "eslint": "^8.24.0",
    "eslint-config-next": "12.3.1",
    "typescript": "4.8.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:types && yarn lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn eslint",
      "yarn stylelint \"app/**/*.js\"",
      "yarn test --bail --findRelatedTests"
    ],
    "*.jsx": [
      "yarn eslint",
      "yarn stylelint \"app/**/*.jsx\"",
      "yarn test --bail --findRelatedTests"
    ],
    "*.ts": [
      "yarn eslint",
      "yarn stylelint \"app/**/*.ts\"",
      "yarn test --bail --findRelatedTests"
    ],
    "*.tsx": [
      "yarn eslint",
      "yarn stylelint \"app/**/*.tsx\"",
      "yarn test --bail --findRelatedTests"
    ]
  }
}
